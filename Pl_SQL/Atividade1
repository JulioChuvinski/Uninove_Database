CREATE OR REPLACE TYPE ALUNO_OBJ AS OBJECT ( -- CRIANDO O OBJETO
 RA CHAR(9),
 NOME VARCHAR2(50));
/
 
DECLARE
 TYPE ALUNOTAB IS TABLE OF ALUNO_OBJ INDEX BY BINARY_INTEGER;
 V_ALUNOS ALUNOTAB;
BEGIN
-- INICIALIZANDO O TIPO DE OBJETO
 V_ALUNOS(1) := ALUNO_OBJ(NULL,NULL); V_ALUNOS(1).RA := '222333444';
 V_ALUNOS(1).NOME := 'Beatriz';
DBMS_OUTPUT.PUT_LINE('RA: ' || V_ALUNOS(1).RA || ' NOME: ' || V_ALUNOS(1).NOME);
END;
/